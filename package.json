{
  "name": "vue-clear-on-disable",
  "description": "A small package for Vue that clears HTML form fields when they're being disabled",
  "version": "1.0.5",
  "private": false,
  "author": {
    "name": "Jeffrey Vercammen",
    "email": "jeffrey@jeffrey.dev"
  },
  "repository": "https://github.com/JeffreyDotDev/vue-clear-on-disable",
  "bugs": "https://github.com/JeffreyDotDev/vue-clear-on-disable/issues",
  "keywords": [
    "vue",
    "form",
    "clear",
    "disable",
    "v-model",
    "reactive"
  ],
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vue-clear-on-disable.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vue-clear-on-disable.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/vue-clear-on-disable.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "rollup": "^2.41.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "main": "dist/vue-clear-on-disable.umd.js",
  "module": "dist/vue-clear-on-disable.esm.js",
  "unpkg": "dist/vue-clear-on-disable.min.js"
}
